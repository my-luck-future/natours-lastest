{
  "name": "natours",
  "version": "1.0.0",
  "description": "Learning node, express and mongoDB",
  "browserslist": [
    "last 1 Chrome version",
    "last 1 Firefox version"
  ],
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
